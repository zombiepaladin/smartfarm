<!-- Import data file modal -->
<div class="modal fade" id="import-data-dialog" tabindex="-1" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title">Import Data File</h4>
      </div>
      <div class="modal-body">
        <p>ONE FINE DAY</p>
        <%= file_field_tag :data_file %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <button id="import-data-button" type="button" class="btn btn-primary">Import</button>
      </div>
    </div>
  </div>
</div>
